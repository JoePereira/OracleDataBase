SET SERVEROUTPUT ON;
DECLARE
    MINHAEXE EXCEPTION;
    n NUMBER := 10;
BEGIN
    FOR i IN &i .. &n LOOP
    DBMS_OUTPUT.PUT_LINE(i * i);
        IF i * 2 = 10 THEN
         RAISE MINHAEXE;
        
        END IF;
    END LOOP;
EXCEPTION
    WHEN MINHAEXE THEN
        RAISE_APPLICATION_ERROR(-20015, 'Você caiu na exceção');
END;